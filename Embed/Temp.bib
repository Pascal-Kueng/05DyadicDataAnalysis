  title = {Daily Psychosocial Determinants of Smoking Cessation: {{Effects}} of a Buddy App Intervention},
  shorttitle = {Daily Psychosocial Determinants of Smoking Cessation},
  author = {Berli, Corina and Schwaninger, Philipp and Scholz, Urte and Lüscher, Janina},
  date = {2025},
  journaltitle = {British Journal of Health Psychology},
  volume = {30},
