@article{huelsnitzEffectsSocialControl2022,
  title = {Effects of {{Social Control}} on {{Eating}} and {{Relational Behaviors}} in {{Romantic Relationships}}},
  author = {Huelsnitz, Chloe O. and Rothman, Alexander J. and Simpson, Jeffry A.},
  date = {2022-11-18},
