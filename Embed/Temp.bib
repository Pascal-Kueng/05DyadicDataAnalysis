@article{luoMediationAnalysisSocial2022,
  title = {Mediation Analysis of Social Isolation and Mortality by Health Behaviors},
  author = {Luo, Juhua and Hendryx, Michael},
  date = {2022-01-01},
  journaltitle = {Preventive Medicine},
  shortjournal = {Preventive Medicine},
  volume = {154},
