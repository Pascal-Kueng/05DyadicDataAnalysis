@article{brownleeRomanticRelationshipsType2024,
  title = {Romantic Relationships and Type 2 Diabetes: {{The}} Role of Invisible Social Control},
  shorttitle = {Romantic Relationships and Type 2 Diabetes},
  author = {Brownlee, Hannah A. and Soriano, Emily C. and Lenhard, M. James and Fenech, Alyssa L. and Morreale, Michael and Siegel, Scott D. and Bolger, Niall and Laurenceau, Jean-Philippe},
