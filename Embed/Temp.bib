@dataset{steyerDevelopmentMultidimensionalMood2003,
  title = {Development of the Multidimensional Mood State Questionnaire (MDBF). Primary dataEntwicklung des Mehrdimensionalen Befindlichkeitsfragebogens (MDBF). Primärdatensatz},
