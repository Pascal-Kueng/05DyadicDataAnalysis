  shorttitle = {The Evolving Field of Digital Mental Health},
  author = {Torous, John and Linardon, Jake and Goldberg, Simon B. and Sun, Shufang and Bell, Imogen and Nicholas, Jennifer and Hassan, Lamiece and Hua, Yining and Milton, Alyssa and Firth, Joseph},
  date = {2025-06},
  journaltitle = {World psychiatry: official journal of the World Psychiatric Association (WPA)},
