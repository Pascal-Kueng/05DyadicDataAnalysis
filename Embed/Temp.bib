  title = {Interpersonal Mechanisms Linking Close Relationships to Health},
  author = {Pietromonaco, Paula R. and Collins, Nancy L.},
  date = {2017-09},
  journaltitle = {American Psychologist},
  shortjournal = {American Psychologist},
  series = {Close {{Family Relationships}} and {{Health}}},
  volume = {72},
  number = {6},
  pages = {531--542},
