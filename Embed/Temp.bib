@article{pattersonIncreaseAngerSymptoms2008,
  title = {Increase in Anger Symptoms after Smoking Cessation Predicts Relapse},
  author = {Patterson, Freda and Kerrin, Kia and Wileyto, E. Paul and Lerman, Caryn},
