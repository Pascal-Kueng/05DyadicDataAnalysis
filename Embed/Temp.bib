@article{borgersEffectDyadicSupport2025,
  title = {The {{Effect}} of a {{Dyadic Support Intervention}} on {{Mindfulness Home Practice Adherence}}: {{A Pilot}} and {{Feasibility Study}}},
  shorttitle = {The {{Effect}} of a {{Dyadic Support Intervention}} on {{Mindfulness Home Practice Adherence}}},
  author = {Borgers, Charlotte and Karremans, Johan C. and Speckens, Anne},
  date = {2025-05-10},
