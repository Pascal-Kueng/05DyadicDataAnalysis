@article{paulyMovingSyncHourly2020,
  title = {Moving in {{Sync}}: {{Hourly Physical Activity}} and {{Sedentary Behavior}} Are {{Synchronized}} in {{Couples}}},
  shorttitle = {Moving in {{Sync}}},
  author = {Pauly, Theresa and Keller, Jan and Knoll, Nina and Michalowski, Victoria I and Hohl, Diana Hilda and Ashe, Maureen C and Gerstorf, Denis and Madden, Kenneth M and Hoppmann, Christiane A},
  date = {2020-01-01},
  journaltitle = {Annals of Behavioral Medicine},
  volume = {54},
