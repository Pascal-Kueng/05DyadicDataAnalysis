@article{labudaPerceptionsPartnerSocial2023,
  title = {Perceptions of Partner Social Control: Accuracy, Bias, and Daily Experience},
  shorttitle = {Perceptions of Partner Social Control},
  author = {LaBuda, Jessica E. and Kochendorfer, Logan B. and Gere, Judith},
  date = {2023-08-03},
  journaltitle = {Psychology \& Health},
  volume = {38},
  number = {8},
