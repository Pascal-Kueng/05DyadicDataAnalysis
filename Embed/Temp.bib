@article{paulyLetsEnjoyEvening2024,
  title = {Let’s {{Enjoy}} an {{Evening}} on the {{Couch}}? {{A Daily Life Investigation}} of {{Shared Problematic Behaviors}} in {{Three Couple Studies}}},
  shorttitle = {Let’s {{Enjoy}} an {{Evening}} on the {{Couch}}?},
  author = {Pauly, Theresa and Lüscher, Janina and Berli, Corina and Hoppmann, Christiane A. and Murphy, Rachel A. and Ashe, Maureen C. and Linden, Wolfgang and Madden, Kenneth M. and Gerstorf, Denis and Scholz, Urte},
