@article{mandyamScopingReviewDigital2025,
  title = {Scoping Review of Digital Health Technologies and Interventions That Target Lifestyle Behavior Change in {{Singapore}}},
  author = {Mandyam, Nikita Kanumoory and Lau, Jacqueline and Keck, Camille and Chen, Elya and Yip, Alexander Wenjun},
  date = {2025-04},
  journaltitle = {DIGITAL HEALTH},
  shortjournal = {DIGITAL HEALTH},
  volume = {11},
  pages = {20552076251337032},
